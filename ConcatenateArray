class Solution {
    public int[] getConcatenation(int[] nums) {
        int size = nums.length;
        int[] ans = new int[2*size];

        for(int i = 0; i < 2*size; i++){
            if(i < size)
                ans[i] = nums[i];
            else
                ans[i] = nums[i-size];
        }

        return ans;
    }
}
